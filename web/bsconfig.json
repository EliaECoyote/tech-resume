{
  "name": "web",
  "version": "0.1.0",
  "bsc-flags": [
    "-bs-super-errors"
  ],
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "__tests__",
      "subdirs": true,
      "type": "dev"
    }
  ],
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "suffix": ".js",
  "bs-dependencies": [
    "reason-react",
    "wonka",
    "bs-fetch",
    "bs-css",
    "bs-pdfjs"
  ],
  "bs-dev-dependencies": [
    "@glennsl/bs-jest"
  ],
  "reason": {
    "react-jsx": 3
  },
  "ppx-flags": [],
  "refmt": 3
}